<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignApplicationNameAndDefaultExpiration">
    <DisplayName>AssignApplicationNameAndDefaultExpiration</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>authRequest.applicationName</Name>
        <Ref>developer.app.name</Ref>
    </AssignVariable>
    <AssignVariable>
        <Name>expiresInSec</Name>
        <Value>3600</Value>
    </AssignVariable>
    <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>