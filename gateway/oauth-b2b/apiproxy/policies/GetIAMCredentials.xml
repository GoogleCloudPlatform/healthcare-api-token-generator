<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations async="false" continueOnError="false" enabled="true" name="GetIAMCredentials" mapIdentifier="iam_credentials">
    <DisplayName>GetIAMCredentials</DisplayName>
    <Properties/>
    <ExpiryTimeInSecs>30</ExpiryTimeInSecs>
    <Get assignTo="private.iam.gcpClientApiSecret">
        <Key>
            <Parameter ref="authRequest.applicationName"/>
        </Key>
    </Get>
    <Scope>environment</Scope>
</KeyValueMapOperations>